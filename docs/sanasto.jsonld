        "@type": "xsd:dateTime"
      },
      "dcterms:modified": {
        "@value": "2026-02-24T17:35",
        "@type": "xsd:dateTime"
      }
    },
    {
      "@id": "https://w3id.org/betk/sanasto/#/concept/pim",
      "@type": "skos:Concept",
      "skos:inScheme": "https://w3id.org/betk/sanasto/rakentamisen-tieto-ja-informaatiomallit",
      "skos:prefLabel": [
