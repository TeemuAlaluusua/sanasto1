    {
      "@id": "https://w3id.org/betk/sanasto/#/concept/informaatio",
      "@type": "skos:Concept",
      "skos:inScheme": "https://w3id.org/betk/sanasto/rakentamisen-tieto-ja-informaatiomallit",
      "skos:prefLabel": [
        {
          "@value": "Informaatio",
          "@language": "fi"
        },
        {
          "@value": "",
          "@language": "sv"
        },
        {
          "@value": "Information",
          "@language": "en"
        }
      ],
      "skos:definition": [
        {
          "@value": "Termillä \"informaatio\" (engl. Information) on perinteisesti viitattu jonkin rakenteen, viestin, merkin tai koodin semanttiseen sisältöön, informaatiosisältöön, joka voidaan tulkita, purkaa tai tehdä jotenkin muuten ilmeneväksi, jos tähän käsittelyyn on saatavilla oikeanlainen\nmenetelmä. Toisin sanoen informaatiolla tarkoitetaan uudelleen tulkittavissa olevaa sovitun muotoista esitystapaa, joka sopii tiedonsiirtoon, viestintään, tulkintaan tai käsittelyyn. Informaatiota voi käsitellä ihminen tai sitä voidaan käsitellä automaattisin menetelmin.\n\n**Lähteet:**\n[ISO 19650-1:2019](https://www.iso.org/)\n[EN/ISO 19650:2018](https://www.iso.org/standard/68078.html)",
          "@language": "fi"
        }
      ],
      "dcterms:source": [
        {
          "@language": "fi"
        }
      ],
      "dcterms:created": {
        "@value": "2026-02-24T17:35",
        "@type": "xsd:dateTime"
      },
      "dcterms:modified": {
        "@value": "2026-02-24T17:35",
        "@type": "xsd:dateTime"
      }
    },
    {
      "@id": "https://w3id.org/betk/sanasto/#/concept/informaatiomalli",
      "@type": "skos:Concept",
      "skos:inScheme": "https://w3id.org/betk/sanasto/rakentamisen-tieto-ja-informaatiomallit",
      "skos:prefLabel": [
        {
          "@value": "informaatiomalli",
          "@language": "fi"
        },
        {
          "@value": "informationsmodell",
          "@language": "sv"
        },
        {
          "@value": "information model",
          "@language": "en"
        }
      ],
      "skos:definition": [
        {
          "@value": "tiettyä kohdetta kuvaava malli, jossa joukko tulkittavissa olevaa dataa on esitetty jäsennetysti",
          "@language": "fi"
        }
      ],
      "skos:note": [
        {
          "@value": "Informaatiomalli on ihmisen tulkittavissa ja tietomallien mukaiselta osaltaan koneluettavassa muodossa.",
          "@language": "fi"
        },
        {
          "@value": "Informaatiomalli voi sisältää esimerkiksi geometrisia malleja, luetteloja, tietokantoja, kirjallisia dokumentteja, kuvia, videoleikkeitä ja äänitteitä. Geometrisia malleja voidaan esittää 2- tai 3-ulotteisina, ja niihin sisältyy usein sijaintitietoa.",
          "@language": "fi"
        },
        {
          "@value": "SFS-EN ISO 19650 -standardissa informaatiomalli-käsitteen määritelmässä viitataan informaatiokoosteeseen, koska määritelmä on laadittu mallin muodostamisen näkökulmasta. Informaatiokooste on nimetty pysyvä joukko informaatiota, joka on noudettavissa tiedoston, järjestelmän tai sovelluksen tallennushierarkiasta.",
          "@language": "fi"
        },
        {
          "@value": "Käsitejärjestelmäkaavio: https://sanastokeskus.fi/suomi.fi/rymallit/tietomalli.svg",
          "@language": "fi"
        }
      ],
      "dcterms:source": [
        {
          "@value": "ISO 19650",
          "@language": "fi",
          "@id": "https://iri.suomi.fi/terminology/rymallit/c15"
        }
      ],
