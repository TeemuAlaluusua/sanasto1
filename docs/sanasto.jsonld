      "@type": "skos:Concept",
      "skos:inScheme": "https://w3id.org/betk/sanasto/toimitusketju",
      "skos:prefLabel": [
        {
          "@value": "ylävirta",
          "@language": "fi"
        },
        {
          "@value": "uppströms",
          "@language": "sv"
        },
        {
          "@value": "upstream",
          "@language": "en"
        }
      ],
      "skos:definition": [
        {
          "@value": "Toimitusketjun osa, joka kattaa vaiheet ja toimijat raaka-ainelähteistä ja toimittajista kokoonpanevaan tai tuottavaan yksikköön. Vastakohta alavirralle. Kutsutaan myös hankintapääksi tai hankintamarkkinoiksi.",
          "@language": "fi"
        }
      ],
      "skos:note": [
        {
          "@value": "Termiä käytetään toimitusketjun tarkastelussa kuvaamaan virtausta kohti tuotantoa tai kokoonpanoa. Rakentamisen kontekstissa ylävirta voi tarkoittaa esimerkiksi materiaalien ja komponenttien toimitusketjua raaka-aineista elementtitehtaalle tai työmaalle.",
          "@language": "fi"
        }
      ],
      "dcterms:source": [
