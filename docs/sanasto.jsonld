      "skos:definition": [
        {
          "@value": "Materiaalitarvelaskenta MRP (engl. Manufacturing Resource Planning) on ohjelmistosovellus, joka ohjaa ja aikatauluttaa materiaalivirtoja tuotannon vaatimusten mukaisesti.",
          "@language": "fi"
        }
      ],
