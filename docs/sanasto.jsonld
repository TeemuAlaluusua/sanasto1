    {
      "@id": "https://w3id.org/betk/sanasto/#/concept/rakentamiskohteiden-informaation-mallinnus",
      "@type": "skos:Concept",
      "skos:inScheme": "https://w3id.org/betk/sanasto/rakentamisen-tieto-ja-informaatiomallit",
      "skos:prefLabel": [
        {
          "@value": "rakentamiskohteiden informaation mallinnus",
          "@language": "fi"
        },
        {
          "@value": "",
          "@language": "sv"
        },
        {
          "@value": "BIM",
          "@language": "en"
        }
      ],
      "skos:definition": [
        {
          "@value": "Rakentamiskohteiden informaation mallinnuksella (engl. building Information modelling) tarkoitetaan rakennetun ympäristön omaisuuskohteen yhteisen digitaalisen esityksen käyttöä suunnittelun, rakentamisen ja käytön helpottamiseksi, jotta päätöksenteolle saadaan luotettava perusta HUOM. Rakennettuja omaisuuskohteita ovat mm. rakennukset, sillat, tiet ja tuotantolaitokset.",
          "@language": "fi"
        }
      ],
      "dcterms:source": [
