      "@type": "skos:Concept",
      "skos:inScheme": "https://w3id.org/betk/sanasto/rakentamisen-tieto-ja-informaatiomallit",
      "skos:prefLabel": [
        {
          "@value": "digitaalinen kaksonen",
          "@language": "fi"
        },
        {
          "@value": "digital tvilling",
          "@language": "sv"
        },
        {
          "@value": "digital twin",
          "@language": "en"
        }
      ],
      "skos:altLabel": [
        {
          "@value": "digikaksonen",
          "@language": "fi"
        }
      ],
      "skos:definition": [
        {
          "@value": "digitaalinen esitys, joka mahdollistaa tietojen synkronisoinnin kohteen fyysisen ja digitaalisen tilan välillä tarpeen mukaan",
          "@language": "fi"
        }
      ],
      "skos:note": [
        {
          "@value": "Digitaalisen kaksosen avulla voi hallita, muuttaa ja jakaa sen fyysiseen vastineeseen liittyvää tietoa erilaisia käyttötarkoituksia varten. Rakennuskohteen digitaalinen kaksonen voi sisältää kohteen elinkaaren eri vaiheissa käytettävien rakennuskohteen informaatiomallien tietoja. Rakennuksen käyttövaiheen digitaalinen kaksonen voi sisältää esimerkiksi rakennuksen ajantasaiset kulutustiedot ja -historian sekä tietoa rakennukseen tehdyistä korjauksista ja muutostöistä koko rakennuksen elinkaaren ajalta.",
          "@language": "fi"
        }
      ],
