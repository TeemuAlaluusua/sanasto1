    {
      "@id": "https://w3id.org/betk/sanasto/#/concept/pilvilaskenta",
      "@type": "skos:Concept",
      "skos:inScheme": "https://w3id.org/betk/sanasto/rakentamisen-tieto-ja-informaatiomallit",
      "skos:prefLabel": [
        {
          "@value": "Pilvilaskenta",
          "@language": "fi"
        },
        {
          "@value": "",
          "@language": "sv"
        },
        {
          "@value": "cloud computing",
          "@language": "en"
        }
      ],
      "skos:definition": [
        {
          "@value": "Pilvilaskenta (engl. cloud computing) on paradigma eli ajatusmalli, jossa mahdollistetaan verkkopohjainen pääsy skaalautuviin ja joustaviin fyysisiin tai virtuaalisiin jaettavissa oleviin resursseihin (palvelimet, käyttöjärjestelmät, verkot, ohjelmistot, sovellukset ja tallennusvälineet) ja jossa palvelun resurssien käyttöä kyetään hallitsemaan itse tarvepohjaisesti.",
          "@language": "fi"
        }
      ],
      "dcterms:source": [
        {
          "@value": "[ISO/IEC 22123-1:2023](https://www.iso.org/standard/82758.html)",
          "@language": "fi"
        }
