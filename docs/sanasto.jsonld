      ],
      "skos:definition": [
        {
          "@value": "fyysinen kokonaisuus, jota parhaillaan rakennetaan tai joka on rakennettu",
          "@language": "fi"
        }
      ],
      "skos:note": [
        {
          "@value": "Infrarakentamisessa rakennuskohteita voidaan kutsua infrakohteiksi. Niitä ovat esimerkiksi liikenneväylät, padot ja satamat.",
          "@language": "fi"
        },
        {
          "@value": "Talonrakennuksessa rakennuskohteita ovat esimerkiksi rakennukset, rakennelmat ja niiden ulkoalueet.",
          "@language": "fi"
        },
        {
          "@value": "Käsitejärjestelmäkaavio: https://sanastokeskus.fi/suomi.fi/rymallit/tietomalli.svg",
          "@language": "fi"
        }
      ],
